<?xml version="1.0" encoding="utf-8"?>
    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/constraintLayoutZaIzdavaceeFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragments.Kolekcije.DigitalnaBiblioteka">

        <FrameLayout
            android:id="@+id/fragment_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints">

            <Button
                android:id="@+id/o_nama_title"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/kolekcija"
                android:text="@string/str_za_korisnike_title"
                android:textColor="@color/white"
                android:textSize="@dimen/over_txt_size"/>

        </FrameLayout>

    <TextView
        android:id="@+id/str_za_izdavace_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:textColor="@color/white"
        android:background="@color/colorPrimary"
        android:paddingTop="@dimen/v8"
        android:paddingBottom="@dimen/v8"
        android:text="subt_dig_bibl"
        android:textAlignment="center"
        android:textSize="@dimen/title_size"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_container" />


        <ScrollView
            android:id="@+id/home_scroll"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/str_za_izdavace_title"
            app:layout_constraintVertical_bias="1.0">

            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">



                <TextView
                    android:id="@+id/vb_body1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="subt_virt"
                    android:textColor="@color/cardview_dark_background"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/link"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:layout_marginBottom="@dimen/v24"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="http://dlib.me"
                    android:textColor="@color/gold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/vb_body1" />

                <Button
                    android:id="@+id/link1"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/v16"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="http://www.nbcg-digitalnabibliografija.me"
                    android:textColor="@color/gold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link" />
                <Button
                    android:id="@+id/link2"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/v16"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="http://www.nbcg-digitalnabibliografija.me"
                    android:textColor="@color/gold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link1" />
                <Button
                    android:id="@+id/link3"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/v16"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="http://www.nbcg-digitalnabibliografija.me"
                    android:textColor="@color/gold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link2" />
                <Button
                    android:id="@+id/link4"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/v16"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="http://www.nbcg-digitalnabibliografija.me"
                    android:textColor="@color/gold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link3" />
                <Button
                    android:id="@+id/link5"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/v16"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="http://www.nbcg-digitalnabibliografija.me"
                    android:textColor="@color/gold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link4" />
                <Button
                    android:id="@+id/link6"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/v16"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="http://www.nbcg-digitalnabibliografija.me"
                    android:textColor="@color/gold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link5" />
                <Button
                    android:id="@+id/link7"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/v16"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="http://www.nbcg-digitalnabibliografija.me"
                    android:textColor="@color/gold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link6" />
                <Button
                    android:id="@+id/link8"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/v16"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="http://www.nbcg-digitalnabibliografija.me"
                    android:textColor="@color/gold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link7" />

                <TextView
                    android:id="@+id/vb_body2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="subt_virt"
                    android:textColor="@color/cardview_dark_background"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link8"/>

                <Button
                    android:id="@+id/link9"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/v16"
                    android:paddingTop="0dp"
                    android:layout_marginStart="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:gravity="left"
                    android:background="@android:color/transparent"
                    android:text="www.nbcg-digitalnabibliografija.me"
