<?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/constraintLayoutZaIzdavaceeFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:context=".fragments.Usluge.ZaKorisnike">

        <ScrollView
            android:id="@+id/home_scroll"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <FrameLayout
                    android:id="@+id/fragment_container"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/header"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingConstraints">


                    <Button
                        android:id="@+id/o_nama_title"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/usluge"
                        android:text="@string/str_za_korisnike_title"
                        android:textColor="@color/white"
                        android:textSize="@dimen/over_txt_size"/>

                </FrameLayout>

                <TextView
                    android:id="@+id/str_za_izdavace_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:textColor="@color/white"
                    android:background="@color/colorPrimary"
                    android:paddingTop="@dimen/v8"
                    android:paddingBottom="@dimen/v8"
                    android:text="@string/str_za_korisnike_title"
                    android:textAlignment="center"
                    android:textSize="@dimen/title_size"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fragment_container"
                    android:gravity="center_horizontal" />

                <FrameLayout
                    android:id="@+id/v_flipper"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/header"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/str_za_izdavace_title"
                    tools:ignore="MissingConstraints">


                    <ViewFlipper
                        android:id="@+id/v_flipper_za_korisnike"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"></ViewFlipper>


                </FrameLayout>


                <TextView
                    android:id="@+id/vb_body1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@string/title_kontakt"
                    android:textColor="@color/cardview_dark_background"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/v_flipper" />

                <Button
                    android:id="@+id/link1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@android:color/transparent"
                    android:textColor="@color/gold"
                    android:layout_marginBottom="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:layout_marginRight="@dimen/v8"
                    android:text="pretraga"
                    android:gravity="left|center"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/vb_body1" />

                <TextView
                    android:id="@+id/vb_body2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@string/title_kontakt"
                    android:textColor="@color/cardview_dark_background"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/link1" />

                <Button
                    android:id="@+id/link2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@android:color/transparent"
                    android:textColor="@color/gold"
                    android:layout_marginBottom="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:layout_marginRight="@dimen/v8"
                    android:text="pretraga"
                    android:gravity="left|center"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/vb_body2" />

                <TextView
                    android:id="@+id/vb_body3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:text="@string/title_kontakt"
                    android:textColor="@color/cardview_dark_background"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/link2" />

                <Button
                    android:id="@+id/link3"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@android:color/transparent"
                    android:textColor="@color/gold"
                    android:layout_marginTop="@dimen/v8"
                    android:layout_marginBottom="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:layout_marginRight="@dimen/v8"
                    android:text="PRAVILNIK O USLOVIMA I NAČINJU KORIŠĆENJA BIBLIOTEČKO-INFORMACIONE GRAĐE I USLUGA"
                    android:gravity="left|center"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/vb_body3" />


                <Button
                    android:id="@+id/link4"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@android:color/transparent"
                    android:textColor="@color/gold"
                    android:layout_marginTop="@dimen/v8"
                    android:layout_marginBottom="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:layout_marginRight="@dimen/v8"
                    android:text="NBCG CJENOVNIK USLUGA"
                    android:gravity="left|center"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link3" />

                <Button
                    android:id="@+id/link5"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@android:color/transparent"
                    android:textColor="@color/gold"
                    android:layout_marginTop="@dimen/v8"
                    android:layout_marginBottom="@dimen/v8"
                    android:layout_marginLeft="@dimen/v8"
                    android:layout_marginRight="@dimen/v8"
                    android:text="ZAHTJEV ZA KOPIRANJE"
                    android:gravity="left|center"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link4" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/link5">

                    <TextView
                        android:id="@+id/podjeli"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="@dimen/v8"
                        android:paddingBottom="@dimen/v8"
                        android:background="@color/gold"
                        android:text="PODELITE"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:gravity="center_horizontal" />

                    <ImageButton
                        android:id="@+id/vb_btn_fb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:background="@drawable/ic_social_facebook_circular_button"
                        android:textSize="@dimen/over_txt_size"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/vb_btn_tw"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/podjeli" />

                    <ImageButton
                        android:id="@+id/vb_btn_tw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:background="@drawable/ic_social_twitter_circular_button"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/vb_btn_ln"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/vb_btn_fb"
                        app:layout_constraintTop_toBottomOf="@+id/podjeli" />

                    <ImageButton
                        android:id="@+id/vb_btn_ln"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:background="@drawable/ic_social_linkedin_circular_button"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/vb_btn_tw"
                        app:layout_constraintTop_toBottomOf="@+id/podjeli" />

                </androidx.constraintlayout.widget.ConstraintLayout>




            </androidx.constraintlayout.widget.ConstraintLayout>

        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>